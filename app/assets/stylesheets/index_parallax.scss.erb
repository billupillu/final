  #header {
  background-color: transparentize(#303030,0.1);
  background-image: -webkit-linear-gradient(top, #303030, #1f1f1f); /* Chrome 10-25, iOS 5+, Safari 5.1+ */
  background-image:         linear-gradient(to bottom, #303030, #1f1f1f); /* Chrome 26, Firefox 16+, IE 10+, Opera */
   width:100vw;
   height: 80px;
       -webkit-box-shadow: 0 12px 8px -8px #00A78B,-0 -12px 8px -8px #00A78B;
       -moz-box-shadow: 0 12px 8px -8px #00A78B,-0 -12px 8px -8px #00A78B;
            box-shadow: 0 12px 8px -8px #00A78B,-0 -12px 8px -8px #00A78B;

   padding-top: 0px;padding-left: 120px;
   position: absolute;
   vertical-align: top;
   margin-top: 24px;
   z-index: +20;
   

}
#header-line{display: inline-block;overflow: hidden;white-space: nowrap;position: relative;}
#header-title{width:300px;float: left;margin-top: 15px;z-index: +24;position: absolute;
margin-left: -150px;left:50%;}
.head-link{text-decoration: none;}.head-link:hover{text-decoration: none;}
.up-down{font-size: 20px;
text-align: center;
letter-spacing: 3px;
width:100px;float: left;}
$k:#899FAE;
#home{color: $k;transition:all 0.4s linear;}
#home:hover{text-decoration: none;color:#f50057;}

#joinus{color: $k;transition:all 0.4s linear;margin-left: 80px;}
#joinus:hover{text-decoration: none;color:#ff3d00;}

#welcome{color: #eeeeee;margin-left: 500px;width: 250px;margin-top: 10px;}

#logout{color: $k;transition:all 0.4s linear;margin-left: 70px;}
#logout:hover{text-decoration: none;color:#ff1744;}

#login{color: $k;transition:all 0.4s linear;margin-left: 550px;}
#login:hover{text-decoration: none;color:#aeea00;}

#logup{color: $k;transition:all 0.4s linear;margin-left: 80px;}
#logup:hover{text-decoration: none;color:#ffd600;}

/*video*/

#play-pause{position: absolute;color:$mg;
  text-shadow:0 0 2px #3c3c3c,-0 -0 2px #3c3c3c,0 0 2px #3c3c3c,-0 -0 2px #3c3c3c,
  0 0 2px #5FCF80, -0 -0 2px #5FCF80;
  margin-top:160px;text-align: center;margin-left: 49vw; 
  transition:all 0.4s linear;

  }
  #play-pause-text{position: absolute;color: #3c3c3c;
    font-size: 80px;font-weight: bold;font-family: ubuntu;
    letter-spacing: 4px;line-height: 1.7em;text-align: center;
    margin-top: 300px;margin-left: 0vw;
    text-shadow:0 0 2px #ffffff,-0 -0 2px #ffffff,0 0 2px #3c3c3c,-0 -0 2px #3c3c3c;
    background-color: #3c3c3c;width:100vw;}

